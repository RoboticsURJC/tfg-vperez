<robot name="g_arm">
		
   

    <link name="base_link">
			<visual>
                <origin xyz="0 0 0.0385" rpy="0 0 0"/>
                <geometry>
                    <cylinder radius="0.07" length="0.077"/>
                </geometry>
                <material name="Cyan">
                    <color rgba="0.0 1.0 1.0 1.0"/>
                </material>
            </visual>
    </link>

    <link name="base_up_link">
		<visual>
            <origin xyz="0.1 0 0.044" rpy="0 0 0"/>
            <geometry>
               <box size="0.117 0.089 0.088"/>
            </geometry>
            <material name="Green">
                <color rgba="0.0 0.1 0.0 1.0"/>
            </material>
        </visual>
		
    </link>

    <link name="first_link">
		<visual>
            <origin xyz="0 0 0.085" rpy="0 0 0"/>
            <geometry>
               <box size="0.02 0.02 0.17"/>
            </geometry>
            <material name="Blue">
                <color rgba="0.0 0.0 0.1 1.0"/>
            </material>
        </visual>
		
    </link>

    

    <joint name="base_joint" type="revolute">
        <axis xyz="0.0 0.0 1"/>
        <origin xyz="0 0 0.077" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="base_up_link"/>
        <limit effort="30" velocity="1.0" lower="-2.2" upper="0.7" />
        
        
    </joint>

    <joint name="firstlink_joint" type="revolute">
        <axis xyz="1 0.0 0.0"/>
        <origin xyz="0 0 0.077" rpy="1.5 0 0"/>
        <parent link="base_up_link"/>
        <child link="first_link"/>
        <limit effort="30" velocity="1.0" lower="-2.2" upper="0.7" />
        
        
    </joint>

</robot>
