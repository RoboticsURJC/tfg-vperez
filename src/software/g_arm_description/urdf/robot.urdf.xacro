      <geometry>
        <mesh filename="package://g_arm_description/meshes/parallel_end_link2.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="blue">
        <color rgba="0.0 0.0 1.0 1.0"/>
      </material>
    </collision>
  </link>

  <joint name="passive_parallel_link2_intermediate" type="revolute">
    <origin
      xyz="0.029054 0.019516 -0.011"
      rpy="0.0 0.0 1.5708" />
    <parent
      link="elbow_link" />
    <child
      link="parallel_end_link2" />
    <axis
      xyz="0.0 0.0 1.0" />
    <limit
      lower="-1.5707"
      upper="1.5707"
      effort="200.0"
      velocity="200.0" />
    <mimic
      joint="joint3"
      multiplier="1.0"/> 
  </joint>

</robot>
