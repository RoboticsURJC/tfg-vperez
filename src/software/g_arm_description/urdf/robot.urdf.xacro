      <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0" />
      <geometry>
        <mesh filename="package://g_arm_description/meshes/parallel_end_link1.dae"/>
      </geometry>
    </visual>
    <collision>
