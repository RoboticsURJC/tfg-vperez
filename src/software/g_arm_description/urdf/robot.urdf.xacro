      <material name="blue">
        <color rgba="0.0 0.0 1.0 1.0"/>
      </material>
    </collision>
  </link>

  <joint name="passive_parallel_link1_joint" type="revolute">
    <origin
      xyz="0.004474 0.058 0.080712"
      rpy="1.5707 0.0 0.0" />
    <parent
      link="motors_link" />
    <child
      link="parallel_end_link1" />
    <axis
      xyz="0.0 0.0 1.0" />
    <limit
      lower="-1.5707"
