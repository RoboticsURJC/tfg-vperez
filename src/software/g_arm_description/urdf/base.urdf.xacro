<?xml version="1.0"?>
<robot name="g_arm">

  <!-- Definition of the base link -->
  <link name="base_link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0" />
      <geometry>
        <mesh filename="package://g_arm_description/meshes/base.dae"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.035" />
      <geometry>
            <cylinder radius="0.07" length="0.07"/>
      </geometry>
      <material name="blue">
        <color rgba="0 0 1 1"/>
      </material>
    </collision>
  </link>

  <!-- Definition of the motors base link -->
  <link name="motors_link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0" />
      <geometry>
        <mesh filename="package://g_arm_description/meshes/motors.dae"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="-0.00975 0 0.0445" />
      <geometry>
            <box size="0.159 0.210 0.089"/> r
      </geometry>
      <material name="blue">
        <color rgba="0 0 1 1"/>
      </material>
    </collision>
  </link>

  <!-- Definition of the arm link 1 -->
  <link name="link1">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0" />
      <geometry>
        <mesh filename="package://g_arm_description/meshes/link1.dae"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0.0 0.0975 0.028" />
      <geometry>
            <box size="0.035 0.195 0.034"/> r
      </geometry>
      <material name="blue">
        <color rgba="0 0 1 1"/>
      </material>
    </collision>
  </link>

  <!-- Definition of the arm link 2 -->
  <link name="link2">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0" />
      <geometry>
        <mesh filename="package://g_arm_description/meshes/link2.dae"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0.0 -0.0675 0.0085" />
      <geometry>
            <box size="0.025 0.230 0.015"/> r
      </geometry>
      <material name="blue">
        <color rgba="0 0 1 1"/>
      </material>
    </collision>
  </link>

  <!-- Definition of the arm end effector -->
  <link name="end_effector">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0" />
      <geometry>
        <mesh filename="package://g_arm_description/meshes/end.dae"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0.0 -0.0675 0.0085" />
      <geometry>
            <box size="0.05 0.05 0.05"/> r
      </geometry>
      <material name="blue">
        <color rgba="0 0 1 1"/>
      </material>
    </collision>
  </link>

    <!-- Definition of the invisible link of the end effector -->
  <link name="invisible_end">

  </link>

  <joint name="joint1" type="revolute">
    <origin
      xyz="0 0 0.076"
      rpy="0 0 0" />
    <parent
      link="base_link" />
    <child
      link="motors_link" />
    <axis
      xyz="0 0 1" />
    <limit
      lower="-1.5707"
      upper="1.5707"
      effort="200.0"
      velocity="200.0" />
  </joint>

  <joint name="joint2" type="revolute">
    <origin
      xyz="0.035 0.0331 0.054"
      rpy="1.5708 0 0" />
    <parent
      link="motors_link" />
    <child
      link="link1" />
    <axis
      xyz="0 0 1" />
    <limit
      lower="-1.5707"
      upper="1.5707"
      effort="200.0"
      velocity="200.0" />
  </joint>

  <joint name="joint3" type="revolute">
    <origin
      xyz="0.0 0.17 0.0362"
      rpy="0 0 1.5708" />
    <parent
      link="link1" />
    <child
      link="link2" />
    <axis
      xyz="0 0 1" />
    <limit
      lower="-1.5707"
      upper="1.5707"
      effort="200.0"
      velocity="200.0" />
  </joint>

  <joint name="passive_joint4_mimic1" type="revolute">
    <origin
      xyz="0.0 -0.17 0.0"
      rpy="0 0 -1.5708" />
    <parent
      link="link2" />
    <child
      link="invisible_end" />
    <axis
      xyz="0 0 1" />
    <limit
      lower="-1.5707"
      upper="1.5707"
      effort="200.0"
      velocity="200.0" />
    <mimic
      joint="joint2"
      multiplier="-1"
    /> 
  </joint>

  <joint name="passive_joint4_mimic2" type="revolute">
    <origin
      xyz="0.0 0.0 0.0"
      rpy="0 0 0" />
    <parent
      link="invisible_end" />
    <child
      link="end_effector" />
    <axis
      xyz="0 0 1" />
    <limit
      lower="-1.5707"
      upper="1.5707"
      effort="200.0"
      velocity="200.0" />
    <mimic
      joint="joint3"
      multiplier="-1"
    />
  </joint>
 



</robot>
