<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="g_arm">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import g_arm urdf file -->
    <xacro:include filename="$(find g_arm_description)/urdf/robot.urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="g_arm.ros2_control.xacro" />


    <xacro:g_arm_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
